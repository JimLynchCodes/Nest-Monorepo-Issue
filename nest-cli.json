{
  "monorepo": true,
  "root": "apps/main",
  "sourceRoot": "apps/main/src",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/main/tsconfig.app.json"
  },
  "projects": {
    "main": {
      "type": "application",
      "root": "apps/main",
      "entryFile": "main",
      "sourceRoot": "apps/main/src",
      "compilerOptions": {
        "tsConfigPath": "apps/main/tsconfig.app.json"
      }
    },
    "my-msvc-1": {
      "type": "application",
      "root": "apps/my-msvc-1",
      "entryFile": "main",
      "sourceRoot": "apps/my-msvc-1/src",
      "compilerOptions": {
        "tsConfigPath": "apps/my-msvc-1/tsconfig.app.json"
      }
    },
    "my-svc-2": {
      "type": "application",
      "root": "apps/my-svc-2",
      "entryFile": "main",
      "sourceRoot": "apps/my-svc-2/src",
      "compilerOptions": {
        "tsConfigPath": "apps/my-svc-2/tsconfig.app.json"
      }
    },
    "my-lib": {
      "type": "library",
      "root": "libs/my-lib",
      "entryFile": "index",
      "sourceRoot": "libs/my-lib/src",
      "compilerOptions": {
        "tsConfigPath": "libs/my-lib/tsconfig.lib.json"
      }
    }
  }
}